    <style>
        @import url('https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@200;300;400;500;600;700;800&family=Space+Grotesk:wght@700&display=swap');
        
        * {
            font-family: 'Plus Jakarta Sans', sans-serif;
        }
        
        .font-display {
            font-family: 'Space Grotesk', sans-serif;
        }
        
        /* Animated Gradient Background */
        .animated-gradient-bg {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 25%, #f093fb 50%, #4facfe 75%, #667eea 100%);
            background-size: 400% 400%;
            animation: gradientShift 20s ease infinite;
        }
        
        @keyframes gradientShift {
            0% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
            100% { background-position: 0% 50%; }
        }
        
        /* Neon Glow Effects */
        .neon-glow {
            animation: neon 1.5s ease-in-out infinite alternate;
        }
        
        @keyframes neon {
            from {
                text-shadow: 0 0 10px #00d4ff, 0 0 20px #00d4ff, 0 0 30px #00d4ff, 0 0 40px #0088ff, 0 0 70px #0088ff, 0 0 80px #0088ff, 0 0 100px #0088ff, 0 0 150px #0088ff;
            }
            to {
                text-shadow: 0 0 5px #00d4ff, 0 0 10px #00d4ff, 0 0 15px #00d4ff, 0 0 20px #0088ff, 0 0 35px #0088ff, 0 0 40px #0088ff, 0 0 50px #0088ff, 0 0 75px #0088ff;
            }
        }
        
        /* 3D Transform */
        .transform-3d {
            transform: perspective(1000px) rotateY(10deg) rotateX(5deg);
            transition: transform 0.5s;
        }
        
        .transform-3d:hover {
            transform: perspective(1000px) rotateY(-10deg) rotateX(-5deg) scale(1.05);
        }
        
        /* Particle Animation */
        .particle {
            position: absolute;
            width: 4px;
            height: 4px;
            background: linear-gradient(135deg, #00d4ff, #0088ff);
            border-radius: 50%;
            animation: floatParticle 15s infinite linear;
        }
        
        @keyframes floatParticle {
            0% {
                transform: translateY(100vh) translateX(0) rotate(0deg);
                opacity: 0;
            }
            10% {
                opacity: 1;
            }
            90% {
                opacity: 1;
            }
            100% {
                transform: translateY(-100vh) translateX(100px) rotate(720deg);
                opacity: 0;
            }
        }
        
        /* Glitch Effect */
        .glitch {
            position: relative;
            animation: glitch 2s infinite;
        }
        
        @keyframes glitch {
            0%, 100% { transform: translate(0); }
            20% { transform: translate(-1px, 1px); }
            40% { transform: translate(1px, -1px); }
            60% { transform: translate(-1px, 1px); }
            80% { transform: translate(1px, -1px); }
        }
        
        .glitch::before,
        .glitch::after {
            content: attr(data-text);
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }
        
        .glitch::before {
            animation: glitch-1 0.5s infinite;
            color: #00d4ff;
            z-index: -1;
        }
        
        .glitch::after {
            animation: glitch-2 0.5s infinite;
            color: #ff00ff;
            z-index: -2;
        }
        
        @keyframes glitch-1 {
            0%, 100% { clip: rect(0, 900px, 0, 0); }
            25% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
            50% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
            75% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
        }
        
        @keyframes glitch-2 {
            0%, 100% { clip: rect(0, 900px, 0, 0); }
            25% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
            50% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
            75% { clip: rect(random(100)px, 9999px, random(100)px, 0); }
        }
        
        /* Liquid Button */
        .liquid-button {
            position: relative;
            padding: 20px 40px;
            display: inline-block;
            text-decoration: none;
            overflow: hidden;
            filter: hue-rotate(0deg);
            transition: filter 0.5s;
        }
        
        .liquid-button:hover {
            filter: hue-rotate(10deg);
        }
        
        .liquid-button::before {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            width: 0;
            height: 0;
            transform: translate(-50%, -50%);
            border-radius: 50%;
            background: rgba(255, 255, 255, 0.5);
            transition: width 0.5s, height 0.5s;
        }
        
        .liquid-button:hover::before {
            width: 300px;
            height: 300px;
        }
        
        /* Typing Effect */
        @keyframes typing {
            from { width: 0; }
            to { width: 100%; }
        }
        
        @keyframes blink {
            50% { border-color: transparent; }
        }
        
        .typing {
            overflow: hidden;
            border-right: 3px solid #00d4ff;
            white-space: nowrap;
            animation: typing 3s steps(40, end), blink 0.75s step-end infinite;
        }
        
        /* Rotating Border */
        .rotating-border {
            position: relative;
            overflow: hidden;
        }
        
        .rotating-border::before {
            content: '';
            position: absolute;
            top: -2px;
            left: -2px;
            right: -2px;
            bottom: -2px;
            background: linear-gradient(45deg, #00d4ff, #0088ff, #00d4ff, #0088ff);
            background-size: 400% 400%;
            z-index: -1;
            animation: rotate-border 3s linear infinite;
        }
        
        @keyframes rotate-border {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }
        
        /* Blob Animation */
        .blob {
            animation: blob 7s infinite;
        }
        
        @keyframes blob {
            0%, 100% {
                transform: translate(0px, 0px) scale(1);
            }
            33% {
                transform: translate(30px, -50px) scale(1.1);
            }
            66% {
                transform: translate(-20px, 20px) scale(0.9);
            }
        }
        
        /* Wave Animation */
        .wave {
            animation: wave 10s linear infinite;
        }
        
        @keyframes wave {
            0% { transform: translateX(0); }
            100% { transform: translateX(-50%); }
        }
        
        /* Bounce Animation */
        .bounce {
            animation: bounce 2s infinite;
        }
        
        @keyframes bounce {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-20px); }
        }
        
        /* Pulse Ring */
        .pulse-ring::before {
            content: '';
            position: absolute;
            inset: -10px;
            border: 2px solid #00d4ff;
            border-radius: inherit;
            animation: pulse-ring 1.5s cubic-bezier(0.215, 0.61, 0.355, 1) infinite;
        }
        
        @keyframes pulse-ring {
            0% {
                transform: scale(0.9);
                opacity: 1;
            }
            100% {
                transform: scale(1.3);
                opacity: 0;
            }
        }
        
        /* Modern Glass */
        .modern-glass {
            background: linear-gradient(135deg, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0.05) 100%);
            backdrop-filter: blur(20px);
            -webkit-backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.2);
            box-shadow: 0 8px 32px rgba(0, 212, 255, 0.2);
        }
        
        /* Shimmer Effect */
        .shimmer {
            background: linear-gradient(105deg, transparent 40%, rgba(255, 255, 255, 0.7) 50%, transparent 60%);
            background-size: 200% 100%;
            animation: shimmer 2s infinite;
        }
        
        @keyframes shimmer {
            0% { background-position: -100% 0; }
            100% { background-position: 100% 0; }
        }
        
        /* Electric Border */
        .electric-border {
            animation: electric 2s infinite;
        }
        
        @keyframes electric {
            0%, 100% {
                box-shadow: 0 0 5px #00d4ff, 0 0 10px #00d4ff, 0 0 15px #00d4ff, 0 0 20px #0088ff;
            }
            50% {
                box-shadow: 0 0 10px #00d4ff, 0 0 20px #00d4ff, 0 0 30px #00d4ff, 0 0 40px #0088ff;
            }
        }
        
        /* Gradient Text */
        .gradient-text {
            background: linear-gradient(90deg, #00d4ff, #0088ff, #00d4ff);
            background-size: 200% auto;
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            animation: gradient-move 3s linear infinite;
        }
        
        @keyframes gradient-move {
            0% { background-position: 0% center; }
            100% { background-position: 200% center; }
        }
        
        /* Hover Lift */
        .hover-lift {
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        }
        
        .hover-lift:hover {
            transform: translateY(-10px) scale(1.02);
            box-shadow: 0 20px 40px rgba(0, 212, 255, 0.3);
        }
        
        /* Sparkle Animation */
        .sparkle {
            position: absolute;
            animation: sparkle 2s infinite;
        }
        
        @keyframes sparkle {
            0%, 100% {
                opacity: 0;
                transform: scale(0);
            }
            50% {
                opacity: 1;
                transform: scale(1);
            }
        }
        
        /* Radar Animation */
        .radar {
            position: relative;
        }
        
        .radar::before,
        .radar::after {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 100%;
            height: 100%;
            border: 2px solid #00d4ff;
            border-radius: 50%;
            animation: radar 2s infinite;
        }
        
        .radar::after {
            animation-delay: 1s;
        }
        
        @keyframes radar {
            0% {
                transform: translate(-50%, -50%) scale(1);
                opacity: 1;
            }
            100% {
                transform: translate(-50%, -50%) scale(2);
                opacity: 0;
            }
        }
        
        /* Custom Scrollbar */
        ::-webkit-scrollbar {
            width: 12px;
        }
        
        ::-webkit-scrollbar-track {
            background: #0a0a0a;
            border: 1px solid #1a1a1a;
        }
        
        ::-webkit-scrollbar-thumb {
            background: linear-gradient(to bottom, #00d4ff, #0088ff);
            border-radius: 6px;
        }
        
        ::-webkit-scrollbar-thumb:hover {
            background: linear-gradient(to bottom, #00a8cc, #0066cc);
        }
    </style>
</head>
